<h1>All Things Near App Academy</h1>

<ul id="reviews">
  <% @reviews.each do |review| %>
    <li>
      <%= link_to review.restaurant, review_path(review), 
      class: 'review_title', id: "review_#{review.id}" %>
    </li>
  <% end %>
  
  <%= link_to "Add a Restaurant", new_review_path, class: "new_review" %>
</ul>