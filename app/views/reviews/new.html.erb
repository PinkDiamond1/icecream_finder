<h1>Add a Restaurant</h1>
<%= form_for(@review) do |f| %>
  <ul>
  <% @review.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :restaurant %><br />
    <%= f.text_field :restaurant %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>